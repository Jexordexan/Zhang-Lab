.SUFFIXES: .f90
LIBS = /usr/lib/liblapack.dylib \
	/usr/lib/libblas.dylib
FIL=svshp.f90 getB.f90 defgrad.f90 mooney.f90 s_pen.f90 s_int.f90 s_kin.f90 getM.f90 main.f90
OBJ= svshp.o getB.o defgrad.o mooney.o s_pen.o s_int.o s_kin.o getM.o main.o
main: $(OBJ)
	gfortran -o main $(OBJ) $(LIBS)
$(OBJ):
	gfortran -c $(FIL)	
clean:
	rm -rf *.out *.o *.mod main
